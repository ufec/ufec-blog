    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4"
  },
  "repository": {
    "url": "https://github.com/ufec/gatsby-theme-xue",
    "type": "git"
  },
  "homepage": "https://github.com/ufec/gatsby-theme-xue#readme",
  "bugs": {
    "url": "https://github.com/ufec/gatsby-theme-xue/issues"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,css}": "prettier --write --ignore-unknown",
    "types/**/*.{d.ts,ts}": "prettier --write --ignore-unknown"
  }
}
